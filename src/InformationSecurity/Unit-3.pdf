    <script type="text/javascript">
        var clearInt;
        var totalMessages = 0;
        var listofConfimationIDs = [];
        //clearInt = setInterval(displayDialog, 5000);



        function getUpdatedMessages() {
            totalMessages = 0;
            $('#divMessage').empty();
            var UserID = '11616140';
               $.ajax({
                   type: "POST",
                   contentType: "application/json; charset=utf-8",
                   url: "WsEmployeeSearch.asmx/MessageDetails",
                   data: JSON.stringify({ EmpCode: UserID }),
                   dataType: "json",
                   success: function (data) {
                       data = data.d;
                       $('#hrdMaximunTime').val('0');
                       var atttable = "<table  class='at-table' ><tbody>";

                       var row = "";
                       for (var i = 0; i < data.length; i++) {
                           totalMessages += 1;
                           $('#hrdMaximunTime').val(data[i].RepeatMessageAfterMiliSecond);
                           listofConfimationIDs.push(data[i].ID);
                           row = '<tr>';
                           row += '<td style="vertical-align:top">' + data[i].SrNo + '</td>';
                           row += '<td style="vertical-align:top">' + data[i].Description + '</td>';
                           row += '</tr>';
                           atttable += row;
                       }

                       atttable += '</tbody></table>';
                       $('#divMessage').html(atttable);

                       if ($('#hrdMaximunTime').val() > 0) {
                           // displayDialog();
                           clearInt = setInterval(displayDialog, $('#hrdMaximunTime').val());
                       }
                       else if (data.length > 0) {
                           displayDialog();
                       }
                       else {

                       }
                   }
                                ,
                   error: function (result) {
                       //alert("Error");
                   }
               });
           }

           function displayDialog() {
               clearInterval(clearInt);
               if ($('#hrdIsUMSBlocked').val() == 'Yes')
                   $('#divUMSBlock').show('fast');
               else
                   $('#divUMSBlock').hide('fast');

               $("#divTesting").dialog
                          ({
                              title: "Important Information for you ",
                              width: 750,
                              resizable: false,
                              modal: true,
                              closeOnEscape: false,
                              dialogClass: 'no-close',
                              draggable: false,
                          });

               $('#divTesting').prev('div').css({ 'color': 'white', 'background-color': 'red' });
               var height = $('#divTesting').css('height');            //$('#ui-id-5').next('a').hide();

               height = height.substr(0, height.length - 2);

               if (height > 400) {
                   $('#divTesting').parent('div').css({ 'margin-top': '0px' });
               }
               else if (height > 300) {
                   $('#divTesting').parent('div').css({ 'margin-top': '0px' });
               }
               else if (height > 200) {
                   $('#divTesting').parent('div').css({ 'margin-top': '40px' });
               }
               else if (height > 150) {
                   $('#divTesting').parent('div').css({ 'margin-top': '60px' });
               }
               else if (height > 125) {
                   $('#divTesting').parent('div').css({ 'margin-top': '80px' });
               }

               else if (height > 100) {
                   $('#divTesting').parent('div').css({ 'margin-top': '100px' });
               }

               else {
                   $('#divTesting').parent('div').css({ 'margin-top': '150px' });
               }
           }



           $(document).ready(function () {
               $('#btnClose').attr('disabled', 'disabled');
               $('#divUMSBlock').hide('fast');

               $('#divTesting').hide();
               var EmpCode = '11616140';

            $('#divMessage').empty();
              var UserID = '';
               UserID = UserID + '11616140';



               $.ajax({
                type: "POST",
                contentType: "application/json; charset=utf-8",
                url: "WsEmployeeSearch.asmx/MessageDetails",
                data: JSON.stringify({ EmpCode: UserID }),
                dataType: "json",
                success: function (data) {
                    data = data.d;
                    $('#hrdMaximunTime').val('0');
                    var atttable = "<table  class='at-table' ><tbody>";

                    var row = "";
                    for (var i = 0; i < data.length; i++) {
                        totalMessages += 1;

                        if (data[i].IsBlocked == 'True') {
                            $('#hrdIsUMSBlocked').val('Yes');

                            $('#hrdMaximunTime').val(data[i].RepeatMessageAfterMiliSecond);
                            listofConfimationIDs.push(data[i].ID);
                            row = '<tr style="font-weight:bold">';
                            row += '<td style="vertical-align:top">' + data[i].SrNo + '</td>';
                            row += '<td style="vertical-align:top" >' + data[i].Description + '</td>';
                            //row += '<td style="vertical-align:top" >' + data[i].IsBlocked + '</td>';
                            row += '</tr>';
                            atttable += row;
                        }
                        else {


                            $('#hrdMaximunTime').val(data[i].RepeatMessageAfterMiliSecond);
                            listofConfimationIDs.push(data[i].ID);
                            row = '<tr>';
                            row += '<td style="vertical-align:top">' + data[i].SrNo + '</td>';
                            row += '<td style="vertical-align:top" >' + data[i].Description + '</td>';
                            //row += '<td style="vertical-align:top" >' + data[i].IsBlocked + '</td>';
                            row += '</tr>';
                            atttable += row;
                        }
                    }

                    atttable += '</tbody></table>';
                    $('#divMessage').html(atttable);


                    if (totalMessages > 0) {
                        displayDialog();
                    } else {
                        ps_SetupPollsContainer('#psTabPollContainer');
                    }
                }
                             ,
                error: function (result) {
                    //alert("Error");
                }
            });


            $('#btnClose').click(function () {

                var userID = '11616140';
                var ID = "";

                for (i = 0; i < listofConfimationIDs.length ; i++) {
                    ID = listofConfimationIDs[i];
                    $.ajax({
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        url: "WsEmployeeSearch.asmx/UpdateMessageDetails",
                        data: JSON.stringify({ Id: ID, EmpCode: userID }),
                        dataType: "json",
                        success: function (data) {

                        }
                    });
                }



                $('#btnClose').removeClass('enableClosedialog');
                $('#btnClose').addClass('disableClosedialog');
                $("#chkReadMessage").attr('checked', false);
                $('#btnClose').attr('disabled', 'disabled');
                $("#divTesting").dialog('close');
                ps_SetupPollsContainer('#psTabPollContainer');

                if ($('#hrdIsUMSBlocked').val() == 'Yes') {


                    window.location.replace('https://ums.lpu.in/lpuums/loginnew.aspx');

                }
                else {
                    getUpdatedMessages();
                }
            });


            $('#chkReadMessage').click(function () {
                if ($(this).is(':checked')) {
                    $('#btnClose').removeAttr('disabled');
                    $('#btnClose').removeClass('disableClosedialog');
                    $('#btnClose').addClass('enableClosedialog');
                }
                else {
                    $('#btnClose').attr('disabled', 'disabled');
                    $('#btnClose').removeClass('enableClosedialog');
                    $('#btnClose').addClass('disableClosedialog');
                }
            });
        });



    </script>

